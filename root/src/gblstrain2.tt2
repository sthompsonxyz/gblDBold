
[% PROCESS mainmenu.tt2 pagename = "Strain Search" %]

<SCRIPT LANGUAGE="JAVASCRIPT">
function clearAll(){
	document.forms[0].reset();
}
</SCRIPT>


<form action="[% c.uri_for('showstrains2') %]" method="GET">
<div id="StrainSearchStrainCode">
Strain Code:<br> <input type="text" name="strainCode" value="" size="10"/>
</div>

<div id="DBIDsearch">
Databse IDs:
</div>

<div id="StrainSearchGBLAccNo">
GBL Accession No.:<br> <input type="text" name="gblAccNo" value="" size="10"/>
</div>

<div id="StrainSearchSample">
